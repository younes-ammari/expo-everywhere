{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "expo-everywhere",
  "version": "1.0.0",
  "identifier": "com.expo.everywhere",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8081",
    "beforeDevCommand": "npm run start:web",
    "beforeBuildCommand": "npm run build:web"
  },
  "app": {
    "windows": [
      {
        "title": "Expo Everywhere",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
